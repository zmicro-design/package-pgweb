#!/bin/bash

export PACKAGE_CORE_NAME="pgweb"
export PACKAGE_CORE_BIN_NAME="$PACKAGE_CORE_NAME"
export PACKAGE_CORE_VERSION=v0.13.1
export PACKAGE_CORE_ORG="sosedoff"

export PACKAGE_CORE_ORG_DOREAMON_DESIGN="doreamon-design"
export PACKAGE_CORE_VERSION_DOREAMON_DESIGN=v0.13.1

if [ "$PLUGIN_PACKAGE_USE_DOREAMON_DESIGN" = "true" ]; then
  export PACKAGE_CORE_ORG=$PACKAGE_CORE_ORG_DOREAMON_DESIGN
  export PACKAGE_CORE_VERSION=$PACKAGE_CORE_VERSION_DOREAMON_DESIGN
fi

# export PACKAGE_CORE_RELEASE_DOWNLOAD=${PACKAGE_CORE_RELEASE_DOWNLOAD:-"https://github.com/${PACKAGE_CORE_ORG}/pgweb/releases/download"}
export PACKAGE_CORE_RELEASE_DOWNLOAD="https://github.com/${PACKAGE_CORE_ORG}/pgweb/releases/download"
